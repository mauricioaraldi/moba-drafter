    "predeploy": "npm run build",
    "serve": "vue-cli-service serve"
  },
  "author": "Mauricio Araldi",
  "keywords": [
    "javascript",
    "moba",
    "draft"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mauricioaraldi/moba-drafter.git"
  },
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/mauricioaraldi/moba-drafter/issues"
  },
  "dependencies": {
    "core-js": "^3.7.0",
    "normalize.css": "^8.0.1",
    "vue": "^2.6.12",
    "vue-gtag": "^1.10.0",
    "vue-router": "^3.4.9"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.3.0",
    "@vue/cli-plugin-eslint": "~4.3.0",
    "@vue/cli-service": "~4.3.0",
    "babel-eslint": "^10.1.0",
    "cross-env": "^7.0.2",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "fibers": "^5.0.0",
    "gh-pages": "^2.2.0",
    "node-sass": "^5.0.0",
    "sass-loader": "^10.1.0",
    "vue-template-compiler": "^2.6.12"
  },
  "eslintConfig": {
